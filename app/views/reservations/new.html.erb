<h2>Rent</h2>

  <%= simple_form_for(@animal) do |f| %>
   <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
   </div>
   <div class="form-actions">
     <%= f.button :submit, class: "btn btn-primary" %>
   </div>
  <% end %>
  <%= link_to 'Back', animal_path, class: "btn btn-ghost" %>
</div>
